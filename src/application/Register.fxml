<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <TextField fx:id="regName" alignment="CENTER" layoutX="81.0" layoutY="16.0" text="Name">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="regAPIKey" alignment="CENTER" layoutX="81.0" layoutY="74.0" text="API Key">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="regUsername" alignment="CENTER" layoutX="81.0" layoutY="172.0" prefHeight="39.0" prefWidth="224.0" text="Username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="regButton" layoutX="156.0" layoutY="325.0" mnemonicParsing="false" onAction="#CreateUser" text="Register">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <PasswordField fx:id="regPassword" alignment="CENTER" layoutX="81.0" layoutY="221.0" prefHeight="39.0" prefWidth="224.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="regPasswordConfirm" alignment="CENTER" layoutX="81.0" layoutY="267.0" prefHeight="39.0" prefWidth="224.0" promptText="Confirm Password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Label fx:id="regStatus" alignment="CENTER" layoutY="22.0" prefHeight="27.0" prefWidth="386.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="regAPISecret" alignment="CENTER" layoutX="81.0" layoutY="124.0" text="API Secret">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
